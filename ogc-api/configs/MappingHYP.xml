  <FeatureTypeMapping>
	<mappingName>_watercourse_hydroid</mappingName>

      <sourceDataStore>idDataStoreInsp</sourceDataStore>
      <sourceType>hyp_hydroid</sourceType>
      <targetElement>hy:HydroIdentifier</targetElement>
      <attributeMappings>
        <AttributeMapping>
          <targetAttribute>FEATURE_LINK[1]</targetAttribute>
          <sourceExpression>
            <OCQL>hyp_object</OCQL>
          </sourceExpression>
        </AttributeMapping>
        <AttributeMapping>
          <targetAttribute>hy:classificationScheme</targetAttribute>
          <sourceExpression>
            <OCQL>hydroidclass</OCQL>
          </sourceExpression>
        </AttributeMapping>
        <AttributeMapping>
          <targetAttribute>hy:localId</targetAttribute>
          <sourceExpression>
            <OCQL>hydroid</OCQL>
          </sourceExpression>
        </AttributeMapping>
        <AttributeMapping>
          <targetAttribute>hy:namespace</targetAttribute>
          <sourceExpression>
            <OCQL>hydroidns</OCQL>
          </sourceExpression>
        </AttributeMapping>
      </attributeMappings>
    </FeatureTypeMapping>
	
	
    <FeatureTypeMapping>
      <sourceDataStore>idDataStoreInsp</sourceDataStore>
      <sourceType>hyp_watercourse</sourceType>
      <targetElement>hy-p:Watercourse</targetElement>
      <attributeMappings>
        <AttributeMapping>
          <targetAttribute>hy-p:Watercourse</targetAttribute>
          <idExpression>
            <OCQL>id</OCQL>
          </idExpression>
        </AttributeMapping>
        <AttributeMapping>
          <targetAttribute>gml:metaDataProperty</targetAttribute>
          <encodeIfEmpty>true</encodeIfEmpty>
          <ClientProperty>
            <name>xlink:href</name>
            <value>metadata</value>
          </ClientProperty>
        </AttributeMapping>
        <AttributeMapping>
          <targetAttribute>gml:identifier</targetAttribute>
          <sourceExpression>
            <OCQL>strconcat(genwcurl, localid)</OCQL>
          </sourceExpression>
          <encodeIfEmpty>true</encodeIfEmpty>
          <ClientProperty>
            <name>codeSpace</name>
            <value>genwcurl</value>
          </ClientProperty>
        </AttributeMapping>
        <AttributeMapping>
          <targetAttribute>hy-p:inspireId/base:Identifier/base:localId</targetAttribute>
          <sourceExpression>
            <OCQL>localid</OCQL>
          </sourceExpression>
        </AttributeMapping>
        <AttributeMapping>
          <targetAttribute>hy-p:inspireId/base:Identifier/base:namespace</targetAttribute>
          <sourceExpression>
            <OCQL>namespace</OCQL>
          </sourceExpression>
        </AttributeMapping>
        <AttributeMapping>
          <targetAttribute>hy-p:inspireId/base:Identifier/base:versionId</targetAttribute>
          <sourceExpression>
            <OCQL>version</OCQL>
          </sourceExpression>
        </AttributeMapping>
        <AttributeMapping>
          <targetAttribute>hy-p:beginLifespanVersion</targetAttribute>
